#define the columns needed to import the dataframe in EnrichmentMap in Cytoscape#
col_oi_EM<-c("term_id","term_name","p_value","FDR","origin","Gene")
#create the dataframe#
dfenr_cyto<-dfenr %>% 
  mutate(origin=1, FDR=p_value) %>%
  rename(Gene=intersection) %>%
  select(all_of(col_oi_EM))
#save the dataframe#
write.table(dfenr_cyto,file="dfenr_cyto.txt",sep="\t",row.names=FALSE,quote = FALSE)

